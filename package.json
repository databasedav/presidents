{
  "name": "presidents",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask": "venv/bin/python src/server/launcher.py",
    "serve": "vue-cli-service serve",
    "build": "rm -rf src/server/static src/server/templates && vue-cli-service build --no-clean",
    "lint": "vue-cli-service lint",
    "start": "npm run flask & npm run serve",
    "setup": "python -m venv venv && venv/bin/pip install -r requirements.txt && npm install"
  },
  "dependencies": {
    "socket.io": "^2.1.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vue-socket.io-extended": "^3.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.8",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.8",
    "@vue/cli-service": "^3.0.0-rc.10",
    "vue-template-compiler": "^2.5.16",
    "webpack-chain": "^4.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
